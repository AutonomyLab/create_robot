<launch>

	<arg name="paused" default="false"/>
	<arg name="gui" default="true"/>
	<arg name="debug" default="false"/>
	<!--arg name="world" default="$(find turtlebot_gazebo)/worlds/corridor.world"/-->
	<arg name="model" default="$(find ca_description)/urdf/create_2_gazebo.urdf.xacro"/>
    
  	<param name="irobot_description" command="$(find xacro)/xacro --inorder $(arg model)" />

	<!-- Launch Gazebo world -->
	<include file="$(find gazebo_ros)/launch/empty_world.launch">
		<!--arg name="world_name" value="$(arg world)"/-->
		<arg name="debug" value="$(arg debug)" />
		<arg name="gui" value="$(arg gui)" />
		<arg name="paused" value="$(arg paused)"/>
		<arg name="use_sim_time" value="true"/>
	</include>

	<node name="urdf_spawner" pkg="gazebo_ros" type="spawn_model" respawn="false" output="screen"
 		  args="-urdf -model my_robot -param irobot_description" />

</launch>